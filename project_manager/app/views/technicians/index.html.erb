<h1>Listing technicians</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Project type</th>
    <th>Skill level</th>
    <th>Supervisor level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @technicians.each do |technician| %>
  <tr>
    <td><%= technician.name %></td>
    <td><%= technician.project_type.to_s %></td>
    <td><%= technician.skill_level.to_s %></td>
    <td><%= technician.supervisor_level.to_s %></td>
    <td><%= link_to 'Show', technician %></td>
    <td><%= link_to 'Edit', edit_technician_path(technician) %></td>
    <td><%= link_to 'Destroy', technician, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Technician', new_technician_path %>
